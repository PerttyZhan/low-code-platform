{
  "name": "low-code-platform",
  "version": "0.0.1",
  "keywords": "vue3,low code",
  "description": "low code platform with vue3",
  "scripts": {
    "dev": "vite --force",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "ssg-build": "vite-ssg build",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.config.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.9",
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "big.js": "^6.2.1",
    "deepmerge": "^4.2.2",
    "echarts": "^5.3.3",
    "element-plus": "^2.2.13",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.18",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@iconify-json/fluent": "^1.1.16",
    "@iconify-json/material-symbols": "^1.1.15",
    "@iconify-json/mdi": "^1.1.30",
    "@iconify-json/ph": "^1.1.2",
    "@iconify-json/tabler": "^1.1.29",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/big.js": "^6.1.5",
    "@types/js-cookie": "^3.0.2",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.47",
    "@unocss/preset-icons": "^0.45.9",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "critters": "^0.0.16",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.1",
    "sass": "^1.54.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "unocss": "^0.45.8",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.4",
    "vite-plugin-imp": "^2.3.0",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-images": "^0.6.1",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-ssg": "^0.20.2",
    "vite-ssg-sitemap": "^0.3.2",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.39.5"
  }
